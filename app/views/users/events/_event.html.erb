<% if event.message_id? %>
  <div class="text-bubble incoming">
    <%= event.content %>
  </div>
  <p class="incoming-msg-from">
    <%= event.created_at.strftime("%m/%d/%Y %l:%M %p") %>
  </p>
<% else %>
  <div class="text-bubble outgoing">
    <%= event.content %>
  </div>
  <p class="incoming-msg-to">
    Tony P.
    <span class="provider-name">Provider</span>
    <%= event.created_at.strftime("%m/%d/%Y %l:%M %p") %>
  </p>
<% end %>
